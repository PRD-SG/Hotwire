<%= turbo_frame_tag 'stock' do%>
<div class="container">
  <div class="row g-3">
    <div
      class="
        col-md-12
        title_page
        d-flex
        justify-content-center
        align-items-center
        shadow
        p-3
        mb-3
        mt-3
        rounded
      "
    >
      Stocks#Edit
    </div>
  </div>
  <div class="row g-3">
    <div class="col-md-12">
      <%= render "form", stock: @stock %>
    </div>
  </div>
</div>
<%end%>
<div class="row g-3  mt-2">
  <div class="col-md-12 d-flex justify-content-center">
    <div class="col-md-4 d-flex justify-content-center btn-w">
      <%= link_to '<span class="material-icons">arrow_back</span
      >'.html_safe,root_path ,class: "btn btn-outline-dark d-flex
      justify-content-center"%>
    </div>
  </div>
</div>
